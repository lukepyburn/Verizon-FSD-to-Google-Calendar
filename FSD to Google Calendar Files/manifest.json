  "permissions": ["activeTab", "declarativeContent", "storage","contextMenus", "clipboardRead", "clipboardWrite","webNavigation", "https://reveal.us.fleetmatics.com/fsd/technicians", "https://reveal.fleetmatics.com/fsd/technicians", "https://calendar.google.com/"],
  "options_page": "options.html",
  "icons": {
    "128": "FSDtoGCAL128.png",
    "16": "FSDtoGCAL16.png",
    "48": "FSDtoGCAL48.png" },
    
